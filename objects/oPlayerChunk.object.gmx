  <events>
    <event eventtype="0" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Player Chunk //

Reassembling = false
speed = random_range(750,1500) / room_speed
direction = random_range(oEnemy.direction - 25, oEnemy.direction + 25)
image_angle = direction
        
OriginalX = x
OriginalY = y

InPosition = false
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Chunk Deceleration // 
Acceleration = 5 / room_speed
Deceleration = 25 / room_speed
if Reassembling = false
{
    if speed &gt;= Deceleration
    {
        speed = speed - Deceleration
    } else{
        speed = 0
        Reassembling = true
        direction = direction + 180
    }
} else {
    
    
    speed = speed + Deceleration
    
    if point_distance(x,y,OriginalX,OriginalY) &lt; speed 
    {
    //Original Position reached
    speed = 0
    if InPosition = false
    {
        oGame.ChunksReassembled = oGame.ChunksReassembled + 1
        InPosition = true
        
        audio_play_sound(aChunkArrive,1,false) //audio
    }
    if oGame.ChunksReassembled &gt;= oGame.NumberOfPlayerChunks
    {
        
        audio_play_sound(aChunkReassemble,1,false) //audio
        
        instance_create(x,y,oPlayer)
        with oPlayerChunk {instance_destroy()}
        // With Function --- affects all instanced object variable
        
        oGame.ChunksReassembled = 0
    }
    }
}


</string>
          </argument>
        </arguments>
      </action>
    </event>
  </events>
